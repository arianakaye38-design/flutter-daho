{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (stable port 8081)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "deviceId": "chrome",
      "args": [
        "--web-hostname",
        "127.0.0.1",
        "--web-port",
        "8081",
        "--verbose"
      ]
    },
    {
      "name": "Flutter Web (program) - Chrome 8081",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "args": [
        "--web-hostname",
        "127.0.0.1",
        "--web-port",
        "8081",
        "--verbose"
      ]
    }
  ]
}
